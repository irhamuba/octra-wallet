{
    "manifest_version": 3,
    "name": "Octra Wallet (DEV)",
    "version": "2.0.0-dev",
    "description": "Development version - Console logs enabled",
    "permissions": [
        "storage",
        "alarms"
    ],
    "action": {
        "default_popup": "index.html",
        "default_title": "Octra Wallet DEV"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self' 'unsafe-eval'; object-src 'none';"
    }
}